.benefits-heading.medium-12
  .inner
    .outer
      .inner.benefit-inner
        = benefit.title
        - if benefit.link
          .benefit-link-wrap
            %a.benefit-link{href: benefit.link}= benefit.link_one_text
        - elsif benefit.id == 37
          .benefit-link-wrap
            %a.benefit-link{href: "https://www.identityiq.com/Credit-Reports/ReportHome.aspx?offercode=431128GN&plancode=PLAN5&sid=#{current_user.member_id}"}= benefit.link_one_text
%ul.large-block-grid-2.small-block-grid-1
  - if benefit.sections.any?
    - @sections = benefit.sections
    = render @sections
.clearfix
